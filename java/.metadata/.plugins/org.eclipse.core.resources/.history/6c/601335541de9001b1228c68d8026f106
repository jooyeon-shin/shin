package lesson001;

public class E015_Array2D {

	public static void main(String[] args) {
		int [] arry1d;
		int [][] arry2d;

		arry1d = new int[] {1,2,3};  // 선언된 배열에 값 초기화
		arry2d = new int[][] {
			{1,2,3},
			{4,5,6,},
			{7,8,9}
		};
		// 개별 요소에의 접근은 index 사용
		for(int i = 0; i < arry1d.length ; 1++) {
			System.out.println(arry1d[i]);
		}

	}
}