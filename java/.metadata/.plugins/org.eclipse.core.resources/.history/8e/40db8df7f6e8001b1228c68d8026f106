package lesson001;

public class P1_Alphabet {

	public static void main(String[] args) {
		// check Alphabet using if else
		char c = 'A';
		String output = (c >= 'a' && c<= 'z') || (c>= 'A' && c <= 'Z')
				? c + " is an alphabet."
				? c + " is not an alphabet.";

		System.out.println(output);
	}

}
