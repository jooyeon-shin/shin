package lesson001;

public class E010_srp {

	public static void main(String[] args) {
		{num == 1
				if num == 2:
					print("승리")
					else:
						print("패배")
		}
		{num = 2

				if num=3:
					print("승리")
					else:
						print("패배")
		}
		{num = 1

				if num=2:
					print("승리")
					else:
						print("패배")

		}
	}
}
