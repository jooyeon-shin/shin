package lesson001;

public class P1_Alphabet {

	public static void main(String[] args) {
		// check Alphabet using if else
		char c = '*';

		if( (c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z'))
			System.out.println(c + " is an alphabet.");
		else
			System.out.println(c + " is not an alphabet.");
	}

}
